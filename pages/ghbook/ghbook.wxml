<van-cell-group>
    <!-- <van-cell title="类型：" value="{{type}}" /> -->
    <van-cell title="工作人员：" value="{{server}}" />
    <van-cell title="日期：" value="{{date}}" />

</van-cell-group>
<van-radio-group value="{{ radio }}" bind:change="onChange">
    <van-cell-group>
        <view class="cu-bar bg-white margin-top">
  <view class="action">
    <text class="cuIcon-title text-pink"></text>
    上午
  </view>
</view>
        <van-cell title="08:30~9:30 剩余{{maxNum-remain1}}" clickable data-name="1" bind:click="onClick">
            <van-radio slot="right-icon" name="1" />
        </van-cell>
        <van-cell title="09:30~10:30 剩余{{maxNum-remain2}}" clickable data-name="2" bind:click="onClick">
            <van-radio slot="right-icon" name="2" />
        </van-cell>
        <van-cell title="10:30~11:30 剩余{{maxNum-remain3}}" clickable data-name="3" bind:click="onClick">
            <van-radio slot="right-icon" name="3" />
        </van-cell>
        <view class="cu-bar bg-white margin-top">
  <view class="action">
    <text class="cuIcon-title text-pink"></text>
    下午
  </view>
</view>
        <van-cell title="14:00~15:00 剩余{{maxNum-remain4}}" clickable data-name="4" bind:click="onClick">
            <van-radio slot="right-icon" name="4" />
        </van-cell>
        <van-cell title="15:00~16:00 剩余{{maxNum-remain5}}" clickable data-name="5" bind:click="onClick">
            <van-radio slot="right-icon" name="5" />
        </van-cell>
        <van-cell title="16:00~17:00 剩余{{maxNum-remain6}}" clickable data-name="6" bind:click="onClick">
            <van-radio slot="right-icon" name="6" />
        </van-cell>
    </van-cell-group>
</van-radio-group>
<view class="padding flex flex-direction">
    <button class="cu-btn bg-green margin-tb-sm lg" bindtap="book">预约预约</button>
</view>